#!/bin/bash

ENV=${1:-$(hostname)}
HOSTNAME=$ENV-fs18.dev.coredial.com
CMD='
   phoneId=""; 
   lastPhoneId=""; 
   for phoneId in $(cut -d "[" -f 3 /var/log/asterisk/full | cut -d "]" -f 1); do 
      if [[ $phoneId -ne $lastPhoneId ]]; then 
         lastPhoneId=$phoneId; 
      fi 
   done; 
   echo $lastPhoneId
   ' 
ssh -q $HOSTNAME $CMD 2> /dev/null 
